<!DOCTYPE html>
<html>

<head>
    <!-- swagger -->
    <link href="${request.static_url('{{project}}:static/bower_components/swagger-ui/dist/css/typography.css')}"
    type='text/css' rel="stylesheet" />
    <link href="${request.static_url('{{project}}:static/bower_components/swagger-ui/dist/css/reset.css')}"
    type='text/css' rel="stylesheet" />
    <link href="${request.static_url('{{project}}:static/bower_components/swagger-ui/dist/css/screen.css')}"
    type='text/css' rel="stylesheet" />
    <script src="${request.static_url('{{project}}:static/bower_components/swagger-ui/lib/jquery-1.8.0.min.js')}"></script>
    <script src="${request.static_url('{{project}}:static/bower_components/swagger-ui/lib/jquery.ba-bbq.min.js')}"></script>
    <script src="${request.static_url('{{project}}:static/bower_components/swagger-ui/lib/jquery.slideto.min.js')}"></script>
    <script src="${request.static_url('{{project}}:static/bower_components/swagger-ui/lib/jquery.wiggle.min.js')}"></script>
    <script src="${request.static_url('{{project}}:static/bower_components/swagger-ui/lib/underscore-min.js')}"></script>
    <script src="${request.static_url('{{project}}:static/bower_components/swagger-ui/lib/backbone-min.js')}"></script>
    <script src="${request.static_url('{{project}}:static/bower_components/swagger-ui/lib/handlebars-2.0.0.js')}"></script>
    <script src="${request.static_url('{{project}}:static/bower_components/swagger-ui/lib/highlight.7.3.pack.js')}"></script>
    <script src="${request.static_url('{{project}}:static/bower_components/swagger-ui/lib/marked.js')}"></script>
    <script src="${request.static_url('{{project}}:static/bower_components/swagger-ui/lib/swagger-oauth.js')}"></script>
    <script src="${request.static_url('{{project}}:static/bower_components/swagger-ui/dist/swagger-ui.min.js')}"></script>
</head>

<body>
    <div class="swagger-section container">
        <div id="swagger-ui-container" class="swagger-ui-wrap"></div>
    </div>
    <script type="text/javascript">
        $(function()
        {
            var swaggerUi = new SwaggerUi(
            {
                url: "${request.route_url('swagger_json')}",
                dom_id: "swagger-ui-container",
                validatorUrl: null,
                docExpansion: "list"
            });
            swaggerUi.load();
        });
    </script>
</body>

</html>
